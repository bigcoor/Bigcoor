{
  "middleware": {
    "devtools": {
      "enabled": false,
      "priority": 35,
      "module": {
        "name": "kraken-devtools",
        "arguments": [
          "path:./public",
          "path:./.build",
          {   
            "copier": {
              "module": "kraken-devtools/plugins/copier",
              "files": "**/*"
            }
          }
        ]
      }
    }
  }
}
